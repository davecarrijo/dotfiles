  "version": "1.0.0",
  "maintainers": ["Davecarrijo"],
  "homepage": "https://github.com/davecarrijo/Fn_blender",
  "rules": [
    {
      "description": "Hai HHKB: fn + 1 = keypad1",
      "manipulators": [
        {
          "from": {
            "key_code": "1",
            "modifiers": {
              "mandatory": ["fn"]
            }
          },
          "to": [
            {
              "key_code": "keypad_1"
            }
          ],
          "type": "basic"
        }
      ]
    },
