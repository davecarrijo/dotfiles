      "description": "Fn + E to open finder",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "e",
            "modifiers": {
              "mandatory": ["fn"]
            }
          },
          "to": [
            {
              "shell_command": "open /System/Library/CoreServices/Finder.app"
            }
          ]
        }
      ]
    },
    {
      "description": "Fn + T to open iterm",
